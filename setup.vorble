#!/bin/sh

set -eu

set +u
# If LOADOUT is already set to something, append a space then the existing value
# after the base loadout file.
export LOADOUT="vorble/loadout/base${LOADOUT+ }${LOADOUT}"
export PATH_ENV="${PATH_ENV}${PATH_ENV+:}vorble/env"
export PATH_HOOK="${PATH_HOOK}${PATH_HOOK+:}vorble/hook"
#export PLZHELP=1
set -u

# vorble platform sets up the following users if they are present.
export USERS="keith root vorble"

`dirname $0`/setup.sh.compatible
